{
  "name": "study_groups_endpoint",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "test": "jest --collectCoverage --detectOpenHandles",
    "start": "ts-node --files server.ts",
    "dev": "nodemon --ext ts,js,json --exec npm start",
    "format": "prettier --write **/*.{tsx,jsx,ts,js,html,json}",
    "format:check": "prettier --check **/*.{tsx,jsx,ts,js,html,json}",
    "typeorm": "typeorm-ts-node-commonjs -d ./repo/AppDataSource.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate ./server/repo/AppDataSource.ts",
    "migration:up": "node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run repo/AppDataSource.ts",
    "migration:down": "node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert repo/AppDataSource.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cli-color": "^2.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.32",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "babel-jest": "^28.1.0",
    "jest": "^28.1.0",
    "jest-ts-auto-mock": "^2.1.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "class-validator": "^0.13.2",
    "cli-color": "^2.0.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.0",
    "ts-auto-mock": "^3.6.2",
    "typeorm": "^0.3.6",
    "uuid": "^8.3.2",
    "ws": "^8.6.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        },
        "@babel/preset-typescript"
      ]
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.ts|js",
      "!dist/**",
      "!coverage/**",
      "!__mocks__/**"
    ],
    "preset": "ts-jest",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    },
    "setupFiles": [
      "<rootDir>/config.ts"
    ]
  }
}
